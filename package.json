{
  "name": "hero-helper",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean:build && ng build",
    "build:prod": "npm run clean:build && ng build --prod",
    "clean:build": "rimraf ./dist",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "serve:build": "lite-server -c bs-config.json",
