{
  "name": "hero-helper",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "@std/esm": "all",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/config.js",
    "clean:coverage": "rm -rf coverage/",
    "clean:dist": "rm -rf dist/",
    "e2e": "protractor protractor-config.js",
    "build-data": "node -r @std/esm server/data-builder/main.js",
    "lint": "tslint --format codeFrame --project tslint.json \"src/app/**/*.ts\"; exit 0",
    "postinstall": "node scripts/fix-ngtools.js",
    "prebuild": "npm run clean:dist",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "preserve:build": "npm run build",
    "ng": "ng",
    "serve:build": "lite-server -c bs-config.json",
    "start:server": "nodemon -r @std/esm --watch server --ignore server/data server/server.js",
    "start": "NODE_ENV=development webpack-dev-server --inline --config webpack/config.js --progress --port 3000",
    "test": "jest --clearCache && jest --coverage --runInBand --watch",
    "test:once": "jest --clearCache && jest --runInBand --coverage"
  },
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "5.2.5",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "5.1.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.1.0",
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.5",
    "@angular/language-service": "5.2.5",
    "@ngrx/store-devtools": "5.1.0",
    "@ngtools/webpack": "1.10.0",
    "@std/esm": "0.21.7",
    "@types/jest": "22.1.3",
    "@types/node": "9.4.6",
    "angular-named-lazy-chunks-webpack-plugin": "1.1.0",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "8.0.0",
    "awesome-typescript-loader": "3.4.1",
    "base-href-webpack-plugin": "1.0.2",
    "body-parser": "1.18.2",
    "circular-dependency-plugin": "4.4.0",
    "codelyzer": "4.1.0",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "cssnano": "3.10.0",
    "exports-loader": "0.7.0",
    "express": "4.16.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.3.0",
    "jest-sonar-reporter": "1.3.0",
    "lite-server": "2.3.0",
    "lowdb": "1.0.0",
    "morgan": "1.9.0",
    "node-sass": "4.7.2",
    "nodemon": "1.15.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "postcss-url": "7.3.0",
    "protractor": "5.3.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.12",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.2",
    "to-string-loader": "1.1.5",
    "ts-jest": "22.0.4",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.2.0",
    "url-loader": "0.6.2",
    "uuid": "3.2.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
