{
  "name": "hero-helper",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "@std/esm": "all",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack/config.js",
    "clean:coverage": "rm -rf coverage/",
    "clean:dist": "rm -rf dist/",
    "e2e": "protractor ./config/protractor/config.js",
    "generate-server-data": "node -r @std/esm server/data/generate.js",
    "json-server": "json-server --watch server/db.json --port 3001 --routes server/routes.json",
    "lint": "tslint --format codeFrame --project ./config/tslint/config.json \"src/app/**/*.ts\"; exit 0",
    "prebuild": "npm run clean",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "preserve:build": "npm run build",
    "ng": "ng",
    "serve:build": "lite-server -c config/lite-server/config.json",
    "start:server": "nodemon -r @std/esm --watch server --ignore server/data server/server.js",
    "start": "NODE_ENV=development webpack-dev-server --inline --config config/webpack/config.js --progress --port 3000",
    "test": "karma start ./config/karma/config.js --coverage=true"
  },
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "5.2.5",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "5.1.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.5",
    "@angular/language-service": "5.2.5",
    "@ngtools/webpack": "1.10.0",
    "@std/esm": "0.21.7",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.4.6",
    "angular-named-lazy-chunks-webpack-plugin": "1.1.0",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "8.0.0",
    "awesome-typescript-loader": "3.4.1",
    "base-href-webpack-plugin": "1.0.2",
    "body-parser": "1.18.2",
    "circular-dependency-plugin": "4.4.0",
    "codelyzer": "4.1.0",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "cssnano": "3.10.0",
    "exports-loader": "0.7.0",
    "express": "4.16.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-remap-coverage": "0.1.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.9",
    "lite-server": "2.3.0",
    "lowdb": "1.0.0",
    "morgan": "1.9.0",
    "node-sass": "4.7.2",
    "nodemon": "1.15.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "postcss-url": "7.3.0",
    "protractor": "5.3.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.12",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.2",
    "to-string-loader": "1.1.5",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.2.0",
    "url-loader": "0.6.2",
    "uuid": "3.2.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
